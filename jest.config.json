{
  "moduleFileExtensions": ["ts", "js"],
  "testRegex": "\\.spec.ts$",
  "rootDir": ".",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "globals": {
    "ts-jest": {
      "tsConfig": "tsconfig.jest.json"
    }
  },
  "coverageReporters": ["json", "lcov"],
  "coverageDirectory": "coverage",
  "collectCoverageFrom": ["src/**/*.ts"]
}
